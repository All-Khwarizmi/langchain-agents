{
  "compilerOptions": {
     "outDir": "./dist",
    "target": "es2017",
    "module": "NodeNext", 
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    
  },
  "exclude": ["node_modules"]                              
}
